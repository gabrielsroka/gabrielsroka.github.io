{
  "manifest_version": 2,
  "name": "rockstar extended",
  "version": "0.1",
  "description": "rockstar extended for IGA Support, based on rockstar by Gabriel Sroka",
  "omnibox": {
    "keyword": "rs"
  },
  "page_action": {
    "default_popup": "menu.html"
  },
  "icons": {
    "16": "rockstar_icon_16.png",
    "128": "rockstar_icon_128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["declarativeContent"],
  "content_scripts": [
    {
      "matches": [
        "https://*.okta.com/*",
        "https://*.okta-emea.com/*",
        "https://*.oktapreview.com/*"
      ],
      "js": ["jquery-1.12.4.min.PATCHED.js", "rockstarExtended.js"],
      "css": ["rockstarExtended.css"]
    }
  ]
}
